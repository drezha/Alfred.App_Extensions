tell application "System Events"	set myList to (name of every process)end tellproperty theURL : ""#Safariif myList contains "Safari" then	tell application "Safari"		set theURL to URL of current tab of window 1	end tellend if#Chrome#if myList contains "Google Chrome" then#	tell application "Google Chrome"#		set theURL to URL of active tab of window 1#	end tell#end ifset the clipboard to theURL